{
	"SomeSettings": {
		"SomeValue": "SomeData"
	},

	"Serilog": {
		"WriteTo": [
			{
				"Name": "RollingFile",
				"Args": {
					"pathFormat": "logs\\log-{Date}.txt",
					"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
				}
			}
		],
		"Enrich": [ "FromLogContext" ]
	}
}
