{
  "Serilog": {
    "SubLoggers": [
      {
        "Level": "Information",
        "pathFormat": "C:/Logs/Serilog/%APPLICATION_NAME%/Information/log-{Date}.log"
      },
      {
        "Level": "Warning",
        "pathFormat": "C:/Logs/Serilog/%APPLICATION_NAME%/Warning/log-{Date}.log"
      },
      {
        "Level": "Fatal",
        "pathFormat": "C:/Logs/Serilog/%APPLICATION_NAME%/Fatal/log-{Date}.log"
      }
    ]
  }
}
